        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Electronic_document_management_system"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        WindowStartupLocation="CenterScreen"
        Title="Профиль учетной записи" SizeToContent="WidthAndHeight">
    <Grid>
        <Label Content="Профиль" HorizontalAlignment="Center" Margin="0,10,0,10" VerticalAlignment="Top" FontSize="36"/>
        <StackPanel x:Name="DataPanel" HorizontalAlignment="Left" Margin="10,75,10,20" VerticalAlignment="Top" MinWidth="342"/>
    </Grid>
</Window>
